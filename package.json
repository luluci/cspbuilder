{
	"name": "cspbuilder",
	"publisher": "hoge",
	"displayName": "CS+Builder",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:cspbuilder.cspbuilderView",
		"onCommand:cspbuilder.start",
		"onCommand:cspbuilder.launch"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "CS+ Builder",
				"properties": {
					"cspBuilder.csplus.path": {
						"type": "string",
						"default": "C:\\Program Files (x86)\\Renesas Electronics\\CS+\\CC\\CubeSuite+.exe",
						"description": "Path to CubeSuite+.exe"
					},
					"cspBuilder.Micom.ROMArea": {
						"type": "object",
						"default": {
							"R5F101SL.begin": "0x0000",
							"R5F101SL.end": "0x7FFF"
						},
						"properties": {
							"R5F101SL.begin": {
								"type": "string"
							},
							"R5F101SL.end": {
								"type": "string"
							}
						}
					}
				}
			}
		],
		"commands": [
			{
				"command": "cspbuilder.start",
				"title": "Start CS+Builder"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/supports-color": "^8.1.0",
		"@types/vscode": "^1.55.0",
		"@types/xml2js": "^0.4.8",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"iconv-lite": "^0.6.2",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"xml2js": "^0.4.23"
	}
}
